{
  "name": "@superwebpros/testsuite",
  "version": "1.0.27",
  "description": "TestSuite, a test suite for all SuperWebPros projects",
  "main": "dist/src",
  "scripts": {
    "dev": "turbo dev",
    "pm": "cd apps/playmakers && npm run dev",
    "swp": "cd apps/superwebpros && npm run dev",
    "prepare": "husky",
    "test": "npx playwright test",
    "tsc": "rm -rf ./dist && tsc --build && turbo tsc",
    "tscall": "turbo tsc",
    "publishpm": "turbo publishpm"
  },
  "keywords": [],
  "author": "leandrosavat",
  "license": "ISC",
  "dependencies": {
    "playwright": "^1.12.0",
    "@playwright/test": "^1.42.1",
    "dotenv": "^16.4.5"
  },
  "workspaces": [
    "apps/*"
  ],
  "devDependencies": {
    "husky": "^9.0.11",
    "turbo": "^1.13.0",
    "typescript": "^5.4.3",
    "@types/node": "^20.11.30"
  }
}
